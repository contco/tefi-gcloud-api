{
    "noImplicitAny": false,
    "compilerOptions": {
      "jsx": "preserve",
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "allowJs": false,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": false,
      "resolveJsonModule": true,
      "isolatedModules": true,
    },
    "lib": [
      "es2015"
    ],
    "include": [
      "api",
      "**/*.ts",
      "index.ts"  ],
    "exclude": [
      "node_modules",
      "gateway"
    ]
  }